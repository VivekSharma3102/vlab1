{
    "name": "p1",
    "version": "1.0.0",
    "description": "p1 is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpile-only ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "p1"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@types/express": "^4.17.1",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "mongodb": "^3.3.3",
        "morgan": "^1.9.1",
        "rotating-file-stream": "^1.4.6",
        "ts-node-dev": "^1.1.8",
        "typescript": "^3.6.3"
    }
}
